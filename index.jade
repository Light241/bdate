        link(href='../bower_components/bootstrap/dist/css/bootstrap.min.css', rel='stylesheet', media='all')
        link(href='../dist/bdate.css', rel='stylesheet', media='all')
        link(rel='stylesheet', type='text/css', href='stylesheets/normalize.css', media='screen')
        link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,700', rel='stylesheet', type='text/css')
        link(rel='stylesheet', type='text/css', href='stylesheets/stylesheet.css', media='screen')
        link(rel='stylesheet', type='text/css', href='stylesheets/github-light.css', media='screen')
        script(src='../bower_components/angular/angular.min.js', type='text/javascript')
        script(src='../dist/bdate.js', type='text/javascript')
        script(src='demo.js', type='text/javascript')
        <!--[if lt IE 9]>
        script(src='static/libs/json3/lib/json3.min.js', type='text/javascript')
        title bdate
